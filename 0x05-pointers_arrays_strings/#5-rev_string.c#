#include "main.h"
/**
*rev_string - function that reverse a string
*@s: parameter to print
*Return: Always 0
*/
void rev_string(char *s)
{
char r = s[0];
 int c;
int i;
 int h;
{
c++;
for (i = 0; s[i] != '\0'; i++)
{
  c = 0;
  h = i / 2
 while (h--)
{
r = s[i -z -1];
s[i - z -1] = s[z];
s[z] = r;
z++
}
}
